    "cozy-client": "^41.8.1",
    "cozy-device-helper": "^2.1.0",
    "cozy-doctypes": "1.85.0",
    "cozy-flags": "2.8.7",
    "cozy-intent": "^2.0.2",
    "cozy-interapp": "0.7.4",
    "cozy-logger": "1.9.0",
    "cozy-realtime": "3.14.4",
    "cozy-stack-client": "^41.2.0",
    "cozy-ui": "^91.2.0",
    "emoji-js": "3.7.0",
    "focus-trap-react": "4.0.1",
    "fuse.js": "6.5.3",
    "lodash": "4.17.21",
    "node-fetch": "2.6.7",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
