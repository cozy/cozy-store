{
  "name": "cozy-store",
  "version": "1.8.0",
  "main": "src/index.jsx",
  "scripts": {
    "tx": "tx pull --all || true",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint && yarn tx",
    "build": "yarn build:browser",
    "build:browser": "cozy-scripts build --browser",
    "build:mobile": "cozy-scripts build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cozy-scripts watch --browser",
    "watch:mobile": "cozy-scripts watch --mobile",
    "start": "cozy-scripts start --hot",
    "cozyPublish": "cozy-scripts publish --token $REGISTRY_TOKEN --prepublish downcloud --postpublish mattermost",
    "test": "cozy-scripts test --verbose --coverage",
    "stack:docker": "docker run --rm -it -p 8080:8080 -v \"$(pwd)/build\":/data/cozy-app/app cozy/cozy-app-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-store.git"
  },
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-store/issues"
  },
  "homepage": "https://github.com/cozy/cozy-store#readme",
  "devDependencies": {
    "babel-preset-cozy-app": "1.5.1",
    "cozy-scripts": "1.13.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.15.3",
    "eslint-config-cozy-app": "1.1.12",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "git-directory-deploy": "1.5.1",
    "identity-obj-proxy": "3.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.8.0",
    "react-markdown": "3.6.0",
    "react-test-renderer": "16.8.6",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "sinon": "7.3.1",
    "sinon-stub-promise": "4.0.0",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "cozy-bar": "6.1.2",
    "cozy-client": "6.20.0",
    "cozy-client-js": "0.15.1",
    "cozy-flags": "1.5.9",
    "cozy-realtime": "2.0.7",
    "cozy-ui": "18.20.0",
    "emoji-js": "3.4.1",
    "eslint-config-prettier": "4.1.0",
    "focus-trap-react": "4.0.1",
    "lodash.get": "4.4.2",
    "node-fetch": "^2.3.0",
    "prop-types": "15.7.2",
    "react-proptypes": "1.0.0",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "redux": "3.7.2",
    "redux-raven-middleware": "1.2.0",
    "url-search-params-polyfill": "5.1.0"
  }
}
