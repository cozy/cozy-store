{
  "extends": "cozy-tsconfig",
  "include": ["./src/**/*"],
  "exclude": ["node_modules"],
  "emitDeclarationOnly": false,
  "noEmit": true,
  "compilerOptions": {
    "paths": {
      "ducks/*": ["./src/ducks/*"],
      "lib/*": ["./src/lib/*"],
      "config/*": ["./src/config/*"],
      "styles/*": ["./src/styles/*"]
    }
  },
}
